    {
      "id": "VGz3xJSC1",
      "name": "Animate Rotate Cube",
      "x": -762,
      "y": 50,
      "templateName": "Custom",
      "draft": "module.exports = (node, graph) => {\n  const triggerIn = node.triggerIn(\"in\");\n  const speedIn = node.in(\"speed\", 1);\n  const rotationOut = node.out(\"rotation\", [0, 0, 0]);\n\n  const rotation = [0, 0, 0];\n\n  triggerIn.onTrigger = (props) => {\n    rotation[1] += props.deltaTime * speedIn.value;\n    node.comment = rotation.map((f) => f.toFixed(0));\n    rotationOut.setValue(rotation);\n  };\n};``",
      "params": [
        {
          "name": "in",
          "source": {
            "id": "dSvq7LN6l",
            "port": "out"
          }
        },
        {
          "name": "speed",
          "value": 18.72
        }
      ]
    },
    {
      "id": "c9Urq2UK7",
      "name": "Texture Loader",
      "x": 45,
      "y": 330,
      "templateName": "Custom",
      "draft": "module.exports = (node, graph) => {\n  const { THREE } = graph;\n  const encodings = {\n    Linear: THREE.LinearEncoding,\n    sRGB: THREE.sRGBEncoding,\n  };\n\n  const urlIn = node.in(\"url\", \"\", { type: \"asset\", thumbnails: true });\n  const encodingIn = node.in(\"encoding\", \"sRGB\", {\n    type: \"dropdown\",\n    values: Object.keys(encodings),\n  });\n  const textureOut = node.out(\"texture\", null);\n\n  let textureLoader = new THREE.TextureLoader();\n\n  let texture = null;\n\n  urlIn.onChange = (url) => {\n    if (!url) return;\n    node.comment = url;\n    textureLoader.load(url, (map) => {\n      texture = map;\n      texture.wrapS = THREE.RepeatWrapping;\n      texture.wrapT = THREE.RepeatWrapping;\n      texture.anisotropy = 4;\n      texture.encoding = encodings[encodingIn.value];\n      map.anisotropy = 4;\n      textureOut.setValue(texture);\n    });\n  };\n\n  encodingIn.onChange = (encoding) => {\n    if (texture) {\n      texture.encoding = encodings[encoding];\n      textureOut.setValue(texture);\n    }\n  };\n\n  node.onDestroy = () => {\n    if (texture) {\n      texture.dispose();\n      texture = null;\n    }\n  };\n};\n",
      "params": [
        {
          "name": "url",
          "value": "assets/cc0textures/WoodFloor041_2K_Normal.jpg"
        },
        {
          "name": "encoding",
          "value": "Linear"
        }
      ]
    },
